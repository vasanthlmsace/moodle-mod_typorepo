{{!
    @template mod_typorepo/View the typo3 page.

    Example context (json):
    {
        "reactionurl_dislike" : "http://moodleurl/mod/typorepo/save.php"
    }
}}

<div class='typorepo-load-block'>
    <script>
        var mform1 = parent.document.getElementById('typorepo-load-iframe').closest('.mform');
        var url = {{#quote}}{{url}}{{/quote}};
        if (typeof mform1.url != 'undefined') {
            mform1.url.value = url;
        }
        if (typeof mform1.config_url != 'undefined') {
            mform1.config_url.value = url;
        }
    </script>
    <div class="save-box">
    <p style="font-family: arial, verdana, serif;">{{linksaved}}</p>
    </div>
</div>


